    "@angular/platform-browser-dynamic": "13.2.4",
    "@angular/router": "13.2.4",
    "@duetds/angular": "latest",
    "@duetds/css": "latest",
    "@duetds/icons": "latest",
    "@duetds/tokens": "latest",
    "rxjs": "7.5.4",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.3",
    "@angular-devkit/build-angular": "13.2.5",
